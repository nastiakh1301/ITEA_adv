data = {'One': 'Один', 'Two': 'Два', 'Three': 'Три', 'Four': 'Четыре'}
new_data = []

with open('test_file.txt', 'r') as f1:
    for i in f1:
        i = i.split(' ', 1)
        new_data.append(data[i[0]] + '  ' + i[1])
    print(new_data)

with open('new_test_file.txt', 'w', encoding="utf-8") as f2:
    f2.writelines(new_data)
